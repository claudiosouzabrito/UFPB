#! /usr/bin/vvp
:ivl_version "10.1 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
:vpi_module "v2009";
S_0x55e21a989560 .scope module, "inv_tb" "inv_tb" 2 1;
 .timescale 0 0;
v0x55e21a9a9950_0 .var "a", 3 0;
v0x55e21a9a9a10_0 .var "clk", 0 0;
v0x55e21a9a9ab0_0 .var "errors", 31 0;
v0x55e21a9a9ba0_0 .var "reset", 0 0;
v0x55e21a9a9c60_0 .net "saida", 3 0, L_0x55e21a9a9fd0;  1 drivers
v0x55e21a9a9d70_0 .var "saidaexperado", 3 0;
v0x55e21a9a9e30 .array "testvectors", 15 0, 7 0;
v0x55e21a9a9ef0_0 .var "vectornum", 31 0;
E_0x55e21a97e510 .event negedge, v0x55e21a9a9a10_0;
E_0x55e21a97f020 .event posedge, v0x55e21a9a9a10_0;
S_0x55e21a9896e0 .scope module, "DUV" "inv" 2 6, 3 1 0, S_0x55e21a989560;
 .timescale 0 0;
    .port_info 0 /INPUT 4 "a"
    .port_info 1 /OUTPUT 4 "saida"
L_0x55e21a9a9fd0 .functor NOT 4, v0x55e21a9a9950_0, C4<0000>, C4<0000>, C4<0000>;
v0x55e21a9898b0_0 .net "a", 3 0, v0x55e21a9a9950_0;  1 drivers
v0x55e21a9a9810_0 .net "saida", 3 0, L_0x55e21a9a9fd0;  alias, 1 drivers
    .scope S_0x55e21a989560;
T_0 ;
    %vpi_call/w 2 8 "$readmemb", "../simulation/modelsim/tabela-inv.tv", v0x55e21a9a9e30 {0 0 0};
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x55e21a9a9ef0_0, 0, 32;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x55e21a9a9ab0_0, 0, 32;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55e21a9a9ba0_0, 0, 1;
    %delay 27, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55e21a9a9ba0_0, 0, 1;
    %end;
    .thread T_0;
    .scope S_0x55e21a989560;
T_1 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55e21a9a9a10_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55e21a9a9a10_0, 0, 1;
    %delay 5, 0;
    %jmp T_1;
    .thread T_1;
    .scope S_0x55e21a989560;
T_2 ;
    %wait E_0x55e21a97f020;
    %delay 1, 0;
    %ix/getv 4, v0x55e21a9a9ef0_0;
    %load/vec4a v0x55e21a9a9e30, 4;
    %split/vec4 4;
    %store/vec4 v0x55e21a9a9d70_0, 0, 4;
    %store/vec4 v0x55e21a9a9950_0, 0, 4;
    %jmp T_2;
    .thread T_2;
    .scope S_0x55e21a989560;
T_3 ;
    %wait E_0x55e21a97e510;
    %load/vec4 v0x55e21a9a9ba0_0;
    %inv;
    %flag_set/vec4 8;
    %jmp/0xz  T_3.0, 8;
    %load/vec4 v0x55e21a9a9c60_0;
    %load/vec4 v0x55e21a9a9d70_0;
    %cmp/ne;
    %jmp/0xz  T_3.2, 6;
    %load/vec4 v0x55e21a9a9950_0;
    %vpi_call/w 2 25 "$display", "Error: inputs = %b", S<0,vec4,u4> {1 0 0};
    %vpi_call/w 2 26 "$display", " outputs = %b (%b experado)", v0x55e21a9a9c60_0, v0x55e21a9a9d70_0 {0 0 0};
    %load/vec4 v0x55e21a9a9ab0_0;
    %addi 1, 0, 32;
    %store/vec4 v0x55e21a9a9ab0_0, 0, 32;
    %jmp T_3.3;
T_3.2 ;
    %vpi_call/w 2 29 "$display", "OK: a: %b | saida: %b (%b experado)", v0x55e21a9a9950_0, v0x55e21a9a9c60_0, v0x55e21a9a9d70_0 {0 0 0};
T_3.3 ;
    %load/vec4 v0x55e21a9a9ef0_0;
    %addi 1, 0, 32;
    %store/vec4 v0x55e21a9a9ef0_0, 0, 32;
    %load/vec4 v0x55e21a9a9ef0_0;
    %cmpi/e 16, 0, 32;
    %jmp/0xz  T_3.4, 4;
    %vpi_call/w 2 33 "$display", "%d tests completed with %d errors", v0x55e21a9a9ef0_0, v0x55e21a9a9ab0_0 {0 0 0};
    %vpi_call/w 2 34 "$finish" {0 0 0};
T_3.4 ;
T_3.0 ;
    %jmp T_3;
    .thread T_3;

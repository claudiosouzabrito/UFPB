#! /usr/bin/vvp
:ivl_version "10.1 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
:vpi_module "v2009";
S_0x55a227419720 .scope module, "acc_tb" "acc_tb" 2 1;
 .timescale 0 0;
v0x55a22743ad50_0 .var "a", 3 0;
v0x55a22743ae30_0 .var "b", 3 0;
v0x55a22743af00_0 .var "c", 0 0;
v0x55a22743b000_0 .var "clk", 0 0;
v0x55a22743b0a0_0 .var "errors", 511 0;
v0x55a22743b190_0 .var "reset", 0 0;
v0x55a22743b250_0 .net "saida", 3 0, v0x55a22743abc0_0;  1 drivers
v0x55a22743b310_0 .var "saidaexperado", 3 0;
v0x55a22743b3d0 .array "testvectors", 511 0, 12 0;
v0x55a22743b490_0 .var "vectornum", 511 0;
E_0x55a227409760 .event negedge, v0x55a22743b000_0;
E_0x55a227409b40 .event posedge, v0x55a22743b000_0;
S_0x55a22741aaf0 .scope module, "DUV" "acc" 2 7, 3 1 0, S_0x55a227419720;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk"
    .port_info 1 /INPUT 4 "a"
    .port_info 2 /INPUT 4 "acu"
    .port_info 3 /OUTPUT 4 "saida"
v0x55a227419950_0 .net "a", 3 0, v0x55a22743ad50_0;  1 drivers
v0x55a22743aa10_0 .net "acu", 3 0, v0x55a22743ae30_0;  1 drivers
v0x55a22743aaf0_0 .net "clk", 0 0, v0x55a22743af00_0;  1 drivers
v0x55a22743abc0_0 .var "saida", 3 0;
E_0x55a2273d4af0 .event edge, v0x55a22743aaf0_0, v0x55a227419950_0, v0x55a22743aa10_0;
    .scope S_0x55a22741aaf0;
T_0 ;
    %wait E_0x55a2273d4af0;
    %load/vec4 v0x55a22743aaf0_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 1;
    %cmp/u;
    %jmp/1 T_0.0, 6;
    %dup/vec4;
    %pushi/vec4 1, 0, 1;
    %cmp/u;
    %jmp/1 T_0.1, 6;
    %jmp T_0.2;
T_0.0 ;
    %load/vec4 v0x55a227419950_0;
    %assign/vec4 v0x55a22743abc0_0, 0;
    %jmp T_0.2;
T_0.1 ;
    %load/vec4 v0x55a22743aa10_0;
    %assign/vec4 v0x55a22743abc0_0, 0;
    %jmp T_0.2;
T_0.2 ;
    %pop/vec4 1;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x55a227419720;
T_1 ;
    %vpi_call/w 2 10 "$readmemb", "../simulation/modelsim/tabela-acc.tv", v0x55a22743b3d0 {0 0 0};
    %pushi/vec4 0, 0, 512;
    %store/vec4 v0x55a22743b490_0, 0, 512;
    %pushi/vec4 0, 0, 512;
    %store/vec4 v0x55a22743b0a0_0, 0, 512;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55a22743b190_0, 0, 1;
    %delay 27, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55a22743b190_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_0x55a227419720;
T_2 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55a22743b000_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55a22743b000_0, 0, 1;
    %delay 5, 0;
    %jmp T_2;
    .thread T_2;
    .scope S_0x55a227419720;
T_3 ;
    %wait E_0x55a227409b40;
    %delay 1, 0;
    %ix/getv 4, v0x55a22743b490_0;
    %load/vec4a v0x55a22743b3d0, 4;
    %split/vec4 4;
    %store/vec4 v0x55a22743b310_0, 0, 4;
    %split/vec4 4;
    %store/vec4 v0x55a22743ae30_0, 0, 4;
    %split/vec4 4;
    %store/vec4 v0x55a22743ad50_0, 0, 4;
    %store/vec4 v0x55a22743af00_0, 0, 1;
    %jmp T_3;
    .thread T_3;
    .scope S_0x55a227419720;
T_4 ;
    %wait E_0x55a227409760;
    %load/vec4 v0x55a22743b190_0;
    %inv;
    %flag_set/vec4 8;
    %jmp/0xz  T_4.0, 8;
    %load/vec4 v0x55a22743b250_0;
    %load/vec4 v0x55a22743b310_0;
    %cmp/ne;
    %jmp/0xz  T_4.2, 6;
    %load/vec4 v0x55a22743af00_0;
    %load/vec4 v0x55a22743ad50_0;
    %load/vec4 v0x55a22743ae30_0;
    %vpi_call/w 2 30 "$display", "Error: inputs = %b_%b_%b", S<2,vec4,u1>, S<1,vec4,u4>, S<0,vec4,u4> {3 0 0};
    %vpi_call/w 2 31 "$display", " outputs = %b (%b experado)", v0x55a22743b250_0, v0x55a22743b310_0 {0 0 0};
    %load/vec4 v0x55a22743b0a0_0;
    %addi 1, 0, 512;
    %store/vec4 v0x55a22743b0a0_0, 0, 512;
    %jmp T_4.3;
T_4.2 ;
    %vpi_call/w 2 34 "$display", "OK: c: %b | a: %b | b: %b | saida: %b (%b experado)", v0x55a22743af00_0, v0x55a22743ad50_0, v0x55a22743ae30_0, v0x55a22743b250_0, v0x55a22743b310_0 {0 0 0};
T_4.3 ;
    %load/vec4 v0x55a22743b490_0;
    %addi 1, 0, 512;
    %store/vec4 v0x55a22743b490_0, 0, 512;
    %load/vec4 v0x55a22743b490_0;
    %cmpi/e 512, 0, 512;
    %jmp/0xz  T_4.4, 4;
    %vpi_call/w 2 38 "$display", "%d tests completed with %d errors", v0x55a22743b490_0, v0x55a22743b0a0_0 {0 0 0};
    %vpi_call/w 2 39 "$finish" {0 0 0};
T_4.4 ;
T_4.0 ;
    %jmp T_4;
    .thread T_4;
# The file index is used to find the file name in the following table.
